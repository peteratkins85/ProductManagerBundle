<?php

namespace Cms\ProductManagerBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * CategoryRelations
 */
class CategoryRelations
{

    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $productId;

    /**
     * @var integer
     */
    private $categoryId;

    /**
     * @var \Cms\ProductManagerBundle\Entity\Products
     */
    private $product;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set productId
     *
     * @param integer $productId
     * @return CategoryRelations
     */
    public function setProductId($productId)
    {
        $this->productId = $productId;

        return $this;
    }

    /**
     * Get productId
     *
     * @return integer 
     */
    public function getProductId()
    {
        return $this->productId;
    }

    /**
     * Set categoryId
     *
     * @param integer $categoryId
     * @return CategoryRelations
     */
    public function setCategoryId($categoryId)
    {
        $this->categoryId = $categoryId;

        return $this;
    }

    /**
     * Get categoryId
     *
     * @return integer 
     */
    public function getCategoryId()
    {
        return $this->categoryId;
    }

    /**
     * Set product
     *
     * @param \Cms\ProductManagerBundle\Entity\Products $product
     * @return CategoryRelations
     */
    public function setProduct(\Cms\ProductManagerBundle\Entity\Products $product = null)
    {
        $this->product = $product;

        return $this;
    }

    /**
     * Get product
     *
     * @return \Cms\ProductManagerBundle\Entity\Products 
     */
    public function getProduct()
    {
        return $this->product;
    }
    /**
     * @var \Cms\ProductManagerBundle\Entity\Categories
     */
    private $category;


    /**
     * Set category
     *
     * @param \Cms\ProductManagerBundle\Entity\Categories $category
     * @return CategoryRelations
     */
    public function setCategory(\Cms\ProductManagerBundle\Entity\Categories $category = null)
    {
        $this->category = $category;

        return $this;
    }

    /**
     * Get category
     *
     * @return \Cms\ProductManagerBundle\Entity\Categories 
     */
    public function getCategory()
    {
        return $this->category;
    }
}
