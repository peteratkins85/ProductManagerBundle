<?php

namespace Cms\ProductManagerBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Sizes
 */
class Sizes
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $productCategoryId;

    /**
     * @var string
     */
    private $sizeName;

    /**
     * @var \DateTime
     */
    private $created;

    /**
     * @var integer
     */
    private $createdBy;

    /**
     * @var string
     */
    private $sizeDescription;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set productCategoryId
     *
     * @param integer $productCategoryId
     * @return Sizes
     */
    public function setProductCategoryId($productCategoryId)
    {
        $this->productCategoryId = $productCategoryId;

        return $this;
    }

    /**
     * Get productCategoryId
     *
     * @return integer 
     */
    public function getProductCategoryId()
    {
        return $this->productCategoryId;
    }

    /**
     * Set sizeName
     *
     * @param string $sizeName
     * @return Sizes
     */
    public function setSizeName($sizeName)
    {
        $this->sizeName = $sizeName;

        return $this;
    }

    /**
     * Get sizeName
     *
     * @return string 
     */
    public function getSizeName()
    {
        return $this->sizeName;
    }

    /**
     * Set created
     *
     * @param \DateTime $created
     * @return Sizes
     */
    public function setCreated($created)
    {
        $this->created = $created;

        return $this;
    }

    /**
     * Get created
     *
     * @return \DateTime 
     */
    public function getCreated()
    {
        return $this->created;
    }

    /**
     * Set createdBy
     *
     * @param integer $createdBy
     * @return Sizes
     */
    public function setCreatedBy($createdBy)
    {
        $this->createdBy = $createdBy;

        return $this;
    }

    /**
     * Get createdBy
     *
     * @return integer 
     */
    public function getCreatedBy()
    {
        return $this->createdBy;
    }

    /**
     * Set sizeDescription
     *
     * @param string $sizeDescription
     * @return Sizes
     */
    public function setSizeDescription($sizeDescription)
    {
        $this->sizeDescription = $sizeDescription;

        return $this;
    }

    /**
     * Get sizeDescription
     *
     * @return string 
     */
    public function getSizeDescription()
    {
        return $this->sizeDescription;
    }
}
