<?php

namespace Cms\ProductManagerBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ProductCategoryRelations
 */
class ProductCategoryRelations
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $productId;

    /**
     * @var integer
     */
    private $productCategoryId;

    /**
     * @var integer
     */
    private $primaryCategory;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set productId
     *
     * @param integer $productId
     * @return ProductCategoryRelations
     */
    public function setProductId($productId)
    {
        $this->productId = $productId;

        return $this;
    }

    /**
     * Get productId
     *
     * @return integer 
     */
    public function getProductId()
    {
        return $this->productId;
    }

    /**
     * Set productCategoryId
     *
     * @param integer $productCategoryId
     * @return ProductCategoryRelations
     */
    public function setProductCategoryId($productCategoryId)
    {
        $this->productCategoryId = $productCategoryId;

        return $this;
    }

    /**
     * Get productCategoryId
     *
     * @return integer 
     */
    public function getProductCategoryId()
    {
        return $this->productCategoryId;
    }

    /**
     * Set primaryCategory
     *
     * @param integer $primaryCategory
     * @return ProductCategoryRelations
     */
    public function setPrimaryCategory($primaryCategory)
    {
        $this->primaryCategory = $primaryCategory;

        return $this;
    }

    /**
     * Get primaryCategory
     *
     * @return integer 
     */
    public function getPrimaryCategory()
    {
        return $this->primaryCategory;
    }
    /**
     * @var \Cms\ProductManagerBundle\Entity\Products
     */
    private $products;

    /**
     * @var \Cms\ProductManagerBundle\Entity\ProductCategories
     */
    private $categories;


    /**
     * Set products
     *
     * @param \Cms\ProductManagerBundle\Entity\Products $products
     * @return ProductCategoryRelations
     */
    public function setProducts(\Cms\ProductManagerBundle\Entity\Products $products = null)
    {
        $this->products = $products;

        return $this;
    }

    /**
     * Get products
     *
     * @return \Cms\ProductManagerBundle\Entity\Products 
     */
    public function getProducts()
    {
        return $this->products;
    }

    /**
     * Set categories
     *
     * @param \Cms\ProductManagerBundle\Entity\ProductCategories $categories
     * @return ProductCategoryRelations
     */
    public function setCategories(\Cms\ProductManagerBundle\Entity\ProductCategories $categories = null)
    {
        $this->categories = $categories;

        return $this;
    }

    /**
     * Get categories
     *
     * @return \Cms\ProductManagerBundle\Entity\ProductCategories 
     */
    public function getCategories()
    {
        return $this->categories;
    }
}
